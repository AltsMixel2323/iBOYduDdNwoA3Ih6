            if v and v.Name == "Yama" then
                if CheckMasteryValue(v.Name) < 350 then
                    AutoFSSLabel.SetText(LabelTextAFS ..  "Waiting for " .. v.Name .. " hit 350 Mastery")
                    getgenv().ASWeapon = v.Name
