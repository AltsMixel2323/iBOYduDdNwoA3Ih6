                            if UnloadBoss then
                                Tween(UnloadBoss.HumanoidRootPart.CFrame * CFrame.new(0, Settings.Distance * 2, 0))
                                return
                            else
                                if Settings.AFSHopServer then
                                    HopServer()
                                    wait(5)
                                    return
                                end
                                if Settings.Cuttay then CT_EnableFarm() end
                                AutoFSSLabel.SetText(LabelTextAFS ..  "Waiting for " .. x .. " spawn")
                            end
                        end
