		Concacmain1.Selectable = true
		Concacmain1.Size = UDim2.new(1, 0, 1, 0)
		Concacmain1.BackgroundTransparency = getgenv().UIColor["Background 1 Transparency"]
		table.insert(UpdateCallBack["Background 1 Transparency"],function() 
			Concacmain1.BackgroundTransparency = getgenv().UIColor["Background 1 Transparency"]
		end)
